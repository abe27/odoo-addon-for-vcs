                    <sheet>
                        <group>
                            <field name="product_type_id"/>
                            <field name="product_id"/>
                        </group>
                        <group>
                            <field name="is_active"/>
                        </group>
                        <notebook>
                            <page name="name" string="Unit">
                                <group>
                                    <field name="unit_id"/>
                                </group>
                            </page>
                            <page name="name" string="Whs">
                                <group>
